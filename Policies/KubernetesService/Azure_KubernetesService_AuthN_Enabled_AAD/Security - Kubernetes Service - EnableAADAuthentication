{
    "mode": "All",
    "parameters": {
      "effect": {
        "type": "String",
        "metadata": {
          "displayName": "Effect",
          "description": "The effect determines what happens when the policy rule is evaluated to match"
        },
        "allowedValues": [
          "Audit",
          "Disabled",
          "Deny"
        ],
        "defaultValue": "Audit"
      }
    },
    
    "policyRule": {
      "if": {
        "not":{
            "anyof":[
                {
                    "field": "Microsoft.ContainerService/managedClusters/aadProfile.managed",
                    "equals": "True"
                }
            ]
        }
      },
      "then": {
        "effect": "[parameters('effect')]"
      }
    }
  }